{
    "$schema": "http://json-schema.org/schema#",
        "properties": {
            "data-set": {
                "type": "object",
                "properties": {
                    "data-group": {
                      "type": "string"
                    },
                    "data-type": {
                      "type": "string"
                    }
                }
            },
            "entrypoint": {
                "type": "string",
                "pattern": "^[a-z0-9_\\.]+$"
            },
            "query": {
                "type": "object",
                "properties": {
                    "site_id": {
                        "type": "string"
                    },
                    "api_method": {
                        "type": "string"
                    },
                    "minutes": {
                        "type": "string",
                        "pattern": "^[0-9]+$"
                    }
                }
            },
            "token": {
                "type": "string"
            }
        },
        "required": [
            "data-set",
            "entrypoint",
            "query",
            "token"
        ],
        "title": "Piwik Realtime Collector",
        "type": "object"
}
