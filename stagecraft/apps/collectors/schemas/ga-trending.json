{
    "$schema": "http://json-schema.org/schema#",
    "properties": {
        "data-set": {
          "type": "object",
          "properties": {
              "data-group": {
                "type": "string"
              },
              "data-type": {
                "type": "string"
              }
          }
        },
        "entrypoint": {
            "type": "string",
            "pattern": "^[a-z0-9_\\.]+$"
        },
        "query": {
            "type": "object",
            "properties": {
                "ids": {
                    "type": "string",
                    "pattern": "^[a-z0-9:]+$"
                },
                "metrics": {
                    "type": "string",
                    "pattern": "^[A-Za-z:]+$"
                },
                "filters": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "dimensions": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "token": {
            "type": "string"
        }
    },
    "required": [
        "data-set",
        "entrypoint",
        "query",
        "token"
    ],
    "title": "Google Analytics Realtime Collector",
    "type": "object"
}
